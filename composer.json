	"license": "MIT",
	"authors": [
		{
			"name": "Javier Eguiluz",
			"email": "javiereguiluz@gmail.com"
		},
		{
			"name": "Dmitriy Kravtsov",
			"email": "dmitriy.kravtsov@systemsdk.com"
		}
	],
	"conflict": {
		"easycorp/easy-log-handler": "*"
	},
	"require": {
		"php": ">=8.1",
		"monolog/monolog": "~3.0",
		"symfony/yaml": "^6.1|^7.0",
		"symfony/framework-bundle": "^6.1|^7.0"
	},
	"autoload": {
